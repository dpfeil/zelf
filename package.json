{
  "name": "zelf",
  "version": "0.0.0",
  "description": "Support for yourself",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "devp": "npm-run-all --parallel devp:app devp:server",
    "devp:app": "export $(cat .env.local | xargs) && node server.js",
    "devp:server": "export $(cat .env.local | xargs) && netlify-lambda serve functions",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpfeil/zelf.git"
  },
  "author": "Doug Pfeil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dpfeil/zelf/issues"
  },
  "homepage": "https://github.com/dpfeil/zelf#readme",
  "dependencies": {
    "@blueprintjs/core": "^3.28.1",
    "axios": "^0.19.2",
    "cross-fetch": "^3.0.4",
    "firebase": "^7.14.6",
    "next": "^9.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "prettier"
    ]
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
